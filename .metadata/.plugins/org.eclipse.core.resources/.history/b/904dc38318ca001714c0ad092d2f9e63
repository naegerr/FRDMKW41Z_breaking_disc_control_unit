/*! Bike simulator Service - Configuration */
typedef struct bsConfig_tag
{
    uint16_t    serviceHandle;                 /*!<Service handle */
    uint8_t     writeValue;            /*!<Input report field */
    uint8_t     notifyValue;            /*!<Input report field */
} bsConfig_t;


bleResult_t Ps_Start(psConfig_t *pServiceConfig);
bleResult_t Ps_Stop(psConfig_t *pServiceConfig);
